# AI Drug Discovery - Environment Configuration
# Copy this file to .env and fill in your values

# AWS Configuration
AWS_DEFAULT_REGION=eu-north-1

# MLflow Configuration  
MLFLOW_TRACKING_URI=http://mlflow:5000
MLFLOW_ARTIFACT_ROOT=s3://ai-drug-artifacts/

# S3 Artifacts Bucket (for MLflow model storage)
# This bucket should exist and be accessible by the EC2 instance role
S3_ARTIFACTS_BUCKET=ai-drug-artifacts

# Training Image (set by CI/CD)
TRAINING_IMAGE=ghcr.io/owner/ai-drug-training:latest